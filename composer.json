{
    "name": "HTTP code worker",
    "description": "Example http code worker",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "doctrine/dbal": "^2.7",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/dotenv": "^5.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "doctrine/dbal": "~2.7"
    },
    "autoload": {
        "classmap": [
            "app/helpers",
            "app/repositories",
            "app/services"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
